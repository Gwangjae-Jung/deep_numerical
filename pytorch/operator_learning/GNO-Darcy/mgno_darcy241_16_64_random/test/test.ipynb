   "source": [
    "from functools import wraps\n",
    "\n",
    "\n",
    "def bias1(func):\n",
    "    bias = 1\n",
    "    @wraps(func)\n",
    "    def wrapper(a,b):\n",
    "        return func(a,b) + bias\n",
    "    return wrapper\n",
    "\n",
    "@bias1\n",
    "def add(a,b):\n",
    "    return a+b\n",
    "\n"
   ]
  },
